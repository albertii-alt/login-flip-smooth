{
  "extends": [
    "development"
  ],
  "hints": {
    "axe/forms": [
      "default",
      {
        "select-name": "off",
        "label": "off"
      }
    ],
    "no-inline-styles": "off",
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "backdrop-filter"
        ]
      }
    ],
    "css-prefix-order": "off",
    "axe/aria": [
      "default",
      {
        "aria-valid-attr-value": "off"
      }
    ]
  },
  "browserslist": [
    "defaults",
    "not ie 11",
    "not ios_saf <= 26",
    "not safari <= 26",
    "not chrome <= 112",
    "not samsung <= 28"
  ]
}